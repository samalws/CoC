[mmm = λa:*.λb:a.b;
mmm : ∀a:*.∀b:a.a;
q = ∀a:*.∀b:a.a;
nnn = mmm;
nnn : q;]

[f = λa:*.λb:*.a;
g = λa:*.a;
x = λa:*.a;
t = ∀a:*.*;
x : t;
x : g t;
x : f t t;]

-> = λa:*.λb:*.∀x:a.b;
idT = λa:*.-> a a;
id = λa:*.λx:a.x;
id : ∀a:*.idT a;
nat = ∀a:*.idT (idT a);

const = λa:*.λb:*.λx:a.λy:b.x;
const : ∀a:*.∀b:*.(-> a (-> b a));
ignore = λa:*.λb:*.λx:a.λy:b.y;

zero = λa:*.const (idT a) (idT a) (id a);
zero : nat;

succ = λn:nat.λa:*.λf:(idT a).λx:a.(f x);
succ : -> nat nat;

